left_join(alltrips, shorttrips, by = "depart" )
left_join(alltrips, shorttrips, by = "depart" ) %>% mutate(n.y / n.x * 100)
ggplot(trips, aes(x = DIST, color = primary_purpose)) + geom_density() + scale_x_log10() + facet_wrap(~trip_mode, scales = "free_y")
ggplot(trips, aes(x = DIST, color = primary_purpose)) + geom_density() + scale_x_log10()
ggplot(DBT, aes(x = DIST, color = primary_purpose)) + geom_density() + scale_x_log10()
ggplot(DBT, aes(x = DIST, color = mode)) + geom_density() + scale_x_log10()
