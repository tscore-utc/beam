hist(DBT$DIST)
ggplot(DBT, aes(x = DIST)) + geom_density()
ggplot(DBT, aes(x = DIST)) + geom_density() + scale_x_log10()
DBT
ggplot(DBT, aes(x = DIST, color = purpose)) + geom_density() + scale_x_log10()
ggplot(DBT, aes(x = DIST, color = primary_purpose)) + geom_density() + scale_x_log10()
ggplot(DBT, aes(x = DIST, color = primary_purpose)) + geom_density() + scale_x_log10() + facet_wrap(~trip_mode)
ggplot(DBT, aes(x = DIST, color = primary_purpose)) + geom_density() + scale_x_log10() + facet_wrap(~trip_mode, scales = "free_y")
DBT %>% group_by(trip_mode) %>% tally()
DBT %>% group_by(trip_mode) %>% filter(DIST > 1) %>% tally()
DBT %>% group_by(depart) %>% filter(DIST > 1) %>% tally()
DBT %>% group_by(depart) %>% tally()
trips %>% group_by(depart) %>% tally()
